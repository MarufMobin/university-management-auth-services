{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "warn",
        "no-undef": "warn",
        "no-unused-expressions": "warn",
        "no-unreachable": "warn",
        "@typescript-eslint/consistent-type-definitions": [
            "error",
            "type"
        ]
    },
    "env": {
        "browser": true,
        "es2023": true,
        "node": true
    },
    "globals": {
        "process": "readonly"
    }
}